{
    "name": "o11y-sample-client",
    "description": "Sample LWC application to demonstrate the 'o11y' instrumentation platform",
    "version": "246.0.0",
    "author": "instrumentationteam@salesforce.com",
    "bugs": "https://git.soma.salesforce.com/instrumentation/o11y-sample-app/issues",
    "dependencies": {
        "@lwc/synthetic-shadow": "2.12.1",
        "@salesforce-ux/design-system": "2.21.0",
        "lightning-base-components": "1.18.1-alpha",
        "lwc": "2.45.2",
        "o11y": "246.1.0",
        "o11y_schema": "246.4.0",
        "protobufjs": "6.11.3"
    },
    "engines": {
        "node": ">=14.16.0",
        "npm": ">=7.4.3",
        "yarn": "1.22.19"
    },
    "homepage": "https://git.soma.salesforce.com/instrumentation/o11y-sample-app",
    "keywords": [
        "distributed tracing",
        "instrumentation",
        "o11y",
        "observability",
        "olly",
        "telemetry",
        "lwc"
    ],
    "license": "MIT",
    "lint-staged": {
        "**/*.{css,html,js,json,md,ts,yaml,yml}": [
            "prettier --write"
        ],
        "./src/**/*.js": [
            "eslint"
        ]
    },
    "repository": "instrumentation/o11y-sample",
    "scripts": {
        "init": "cp ../_common/generated/o11y_schema.d.ts ./src/typings",
        "build:production": "NODE_ENV=production webpack",
        "build": "webpack",
        "clean": "rimraf ../../dist-client",
        "lint": "eslint ./src/**/*.ts",
        "prettier": "prettier --write \"**/*.{css,html,js,json,md,ts,yaml,yml}\"",
        "prettier:verify": "prettier --list-different \"**/*.{css,html,js,json,md,ts,yaml,yml}\"",
        "start": "node scripts/server.js",
        "watch": "webpack-dev-server"
    },
    "devDependencies": {
        "@types/estree": "0.0.50",
        "@types/express": "4.17.13",
        "copy-webpack-plugin": "5.1.2",
        "error-overlay-webpack-plugin": "1.1.0",
        "esbuild-loader": "2.18.0",
        "eslint": "7.32.0",
        "fork-ts-checker-webpack-plugin": "7.2.3",
        "html-webpack-plugin": "5.5.0",
        "lint-staged": "10.5.4",
        "lwc-webpack-plugin": "2.1.0",
        "prettier": "2.6.2",
        "rimraf": "5.0.0",
        "webpack": "5.72.0",
        "webpack-cli": "3.3.12",
        "webpack-dev-server": "3.11.3"
    }
}
